
var i = 1.5
while(i > -1.5)
{
    var x = -1.5
    while(x < 1.5)
    {
        var y = x * x + i * i - 1
        var a = (((y * y) * y) - ((((x * x) * i) * i) * i));
        var cmp = (a < 0);
        if(cmp)
        {
            print("*")
        }
        else
        {
            print(" ")
        }
        x = x + 0.05
    }
    i = i - 0.1
    println()
}
